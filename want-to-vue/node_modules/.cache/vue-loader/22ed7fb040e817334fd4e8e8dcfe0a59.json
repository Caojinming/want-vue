mplate>\r\n  <div class=\"app\">\r\n    <!--1.轮播图-->\r\n    <mt-swipe :auto=\"4000\">\r\n        <mt-swipe-item v-for=\"item in list\">\r\n          <img :src=\"item.img_url\">\r\n        </mt-swipe-item>\r\n    </mt-swipe>\r\n    <!--2.六宫格-->\r\n    <ul class=\"mui-table-view mui-grid-view mui-grid-9\">\r\n          <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-4\">\r\n            <router-link to=\"/home/newslist\">\r\n              <img src=\"../../../public/img/menu1.png\">\r\n              <div class=\"mui-media-body\">新闻资讯</div>\r\n            </router-link>\r\n          </li>\r\n          <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-4\">\r\n            <router-link to=\"/home/photo\">\r\n              <img src=\"../../../public/img/menu2.png\">\r\n              <div class=\"mui-media-body\">图片分享</div>\r\n            </router-link>\r\n          </li>\r\n          <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-4\">\r\n            <router-link to=\"/home/goodslist\">\r\n              <img src=\"../../../public/img/menu3.png\">\r\n              <div class=\"mui-media-body\">商品购买</div>\r\n            </router-link>\r\n          </li>\r\n          <br>\r\n          <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-4\">\r\n            <a href=\"#\">\r\n              <img src=\"../../../public/img/menu4.png\">\r\n              <div class=\"mui-media-body\">发表评论</div>\r\n            </a>\r\n          </li>\r\n          <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-4\">\r\n            <a href=\"#\">\r\n              <img src=\"../../../public/img/menu5.png\">\r\n              <div class=\"mui-media-body\">联系我们</div>\r\n            </a>\r\n          </li>\r\n         <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-4\">\r\n           <a href=\"#\">\r\n            <img src=\"../../../public/img/menu6.png\">\r\n            <div class=\"mui-media-body\">自己开店</div>\r\n          </a>\r\n        </li>\r\n      </ul>   \r\n  </div>\r\n</template>\r\n<script>\r\n  //1.加载Toast组件\r\n  import {Toast} from \"mint-ui\";\r\n  //2.创建Vue实例\r\n  export default{\r\n    data(){\r\n      return {\r\n        list:[]\r\n      }\r\n    },\r\n    created(){\r\n      this.getImageList();\r\n    },\r\n    methods:{\r\n      //1.homeContainer.vue 发送ajax请求\r\n      //获取图片列表\r\n      //选项1.axios          新的解决方案(开始使用)\r\n      //选项2.vue-resource   旧的解决方案(大量公司使用)\r\n      getImageList(){\r\n        this.$http.get(\"imagelist/list\")\r\n        .then(result=>{\r\n          var obj=result.body;\r\n          if(obj.code == 1){  //返回成功数据\r\n            this.list=obj.msg;\r\n          }else{  //返回失败数据\r\n            //测试     this.$toast(\"数据加载失败\")  \r\n            Toast(\"数据加载失败\")  //脚手架 \r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style>\r\n  /*父元素高度为0 设置高度 200px*/\r\n  .app .mint-swipe{\r\n    height:200px;\r\n  }\r\n  /*修改图片轮播中图片的样式 自适应宽度*/\r\n  .app .mint-swipe img{\r\n    width:100%;\r\n    height:100%;\r\n  }\r\n  /*修改样式 大小 背景 边框*/\r\n  /*修改图片大小*/\r\n  .app .mui-grid-view.mui-grid-9 img{\r\n    width:60px;\r\n    height:60px;\r\n  }\r\n  /*修改背景颜色*/\r\n  .app .mui-grid-view.mui-grid-9{\r\n      background-color:#fff;\r\n    }\r\n  /*修改六宫格边框*/\r\n  .app .mui-grid-view.mui-grid-9 .mui-table-view-cell{\r\n      border:0px;\r\n    }\r\n</style>"]}]}