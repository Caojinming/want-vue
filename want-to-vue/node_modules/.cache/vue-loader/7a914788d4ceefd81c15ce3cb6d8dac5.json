nt-size:12px;\r\n  margin-left:10px;\r\n}\r\n/*修饰促销*/\r\n.goods-list .goods-item .info .sell{\r\n  display:flex;\r\n  justify-content:space-between;\r\n  font-size:13px;\r\n}\r\n",{"version":3,"sources":["GoodsList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsFA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"GoodsList.vue","sourceRoot":"src\\components\\goods","sourcesContent":["<template>\r\n  <div class=\"goods-list\">\r\n    <!--<router-link :to=\"'/home/goodsinfo/'+item.lid\" class=\"goods-item\" v-for=\"item in list\" :key=\"item.id\">\r\n      <img :src=\"item.img_url\"/>\r\n      <h1 class=\"title\">{{item.title}}</h1>\r\n      <div class=\"info\">\r\n        <div class=\"price\">\r\n          <span class=\"now\">现价¥{{item.price_new}}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n          <span class=\"old\">原价¥{{item.price_old}}</span>\r\n        </div>\r\n        <div class=\"sell\">\r\n          <span>热卖中</span>\r\n          <span>库存{{item.stock}}件</span>\r\n        </div>\r\n      </div>\r\n    </router-link>-->\r\n\r\n    <!--在网页中用编程方式跳转-->\r\n    <div class=\"goods-item\" v-for=\"item in list\" :key=\"item.lid\" @click=\"goDetail(item.lid)\">\r\n      <img :src=\"item.img_url\"/>\r\n      <h1 class=\"title\">{{item.title}}</h1>\r\n      <div class=\"info\">\r\n        <div class=\"price\">\r\n          <span class=\"now\">现价¥{{item.price_new}}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n          <span class=\"old\">原价¥{{item.price_old}}</span>\r\n        </div>\r\n        <div class=\"sell\">\r\n          <span>热卖中</span>\r\n          <span>库存{{item.stock}}件</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <mt-button type=\"default\" size=\"large\" @click=\"getMore\">加载更多</mt-button>\r\n  </div>\r\n</template>\r\n<script>\r\n  import Toast from 'mint-ui'\r\n  export default{\r\n    data(){\r\n      return {\r\n        list:[],\r\n        pno:1\r\n      }\r\n    },\r\n    created(){\r\n      this.getGoodsList();\r\n    },\r\n    methods:{\r\n      getGoodsList(){\r\n        var url='goodslist/list?pno='+this.pno;\r\n        this.$http.get(url)\r\n        .then(result=>{\r\n          if(result.body.code==1){\r\n            this.list=this.list.concat(result.body.msg)\r\n          }else{\r\n            Toast(\"请求失败\")\r\n          }\r\n        })\r\n      },\r\n      getMore(){\r\n        //1.当前页+1\r\n        this.pno++;\r\n        //2.调用查询的方法\r\n        this.getGoodsList();\r\n      },\r\n      /*获取商品的详细信息 */\r\n      goDetail(lid){\r\n        //一定注意区分this.$route和this.router\r\n        //注意：一定要区分this.$route 和 this.router\r\n          //其中：this.$router参数对象，多有参数，params,query都是属于它\r\n          //query  查询字符串\r\n          //用它可以方便使$route导航对象\r\n          //用它可以方便使用js代码实现路由\r\n          //前进 后退 跳转路由\r\n        //1.最简单的跳转：\r\n        //this.$router.push(\"/home/goodsinfo/\"+lid)\r\n        //2.传递对象跳转\r\n        //this.$router.push({path:\"/home/goodsinfo/\"+lid})\r\n        //3.传递命名路由\r\n        this.$router.push({name:\"goodsinfo\",params:{lid}})\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style>\r\n/*商品列表样式处理 */\r\n.goods-list{\r\n  display:flex;\r\n  flex-wrap:wrap;\r\n  justify-content:space-between;\r\n}\r\n.goods-list .goods-item{\r\n  width:46%;\r\n  margin:2%;\r\n  border:2px solid #ccc;\r\n  border-radius:5px;\r\n  box-shadow:0 0 8px #ccc;\r\n  display:flex;\r\n  flex-direction:column;\r\n  justify-content:space-between;\r\n  min-height:293px;\r\n  background:#fff;\r\n}\r\n.goods-list .goods-item .title{\r\n  font-size:16px;\r\n}\r\n.goods-list .goods-item img{\r\n    width:100%;\r\n}\r\n.goods-list .goods-item .info{\r\n  background:#eee;\r\n}\r\n/*修改价格 */\r\n.goods-list .goods-item .info .price .now{\r\n  color:red;\r\n  font-weight:bold;\r\n  font-size:16px;\r\n}\r\n.goods-list .goods-item .info .price .old{\r\n  text-decoration:line-through;\r\n  font-size:12px;\r\n  margin-left:10px;\r\n}\r\n/*修饰促销*/\r\n.goods-list .goods-item .info .sell{\r\n  display:flex;\r\n  justify-content:space-between;\r\n  font-size:13px;\r\n}\r\n</style>"]}]}