t\"></swiper-box>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n    <!--商品购买区域-->\r\n    <div class=\"mui-card\">\r\n\t\t\t<div class=\"mui-card-header\">{{goodsinfo.lname}}</div>\r\n\t\t\t<div class=\"mui-card-content\">\r\n\t\t\t\t<div class=\"mui-card-content-inner\">\r\n\t\t\t\t\t<p class=\"price\">\r\n            市场价<del>¥{{goodsinfo.price_old}}</del>\r\n            折扣价<span class=\"now_price\">¥{{goodsinfo.price_new}}</span>\r\n          </p>\r\n          <p>购买数量\r\n            <div class=\"mui-numbox\">\r\n              <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"minus\" v-show=\"count>1\">-</button>\r\n              <input class=\"mui-input-numbox\" type=\"number\" value=\"1\" v-model=\"count\"/>\r\n              <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"add\">+</button>\r\n            </div>\r\n          </p>\r\n          <p>\r\n            <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n            <mt-button type=\"danger\" size=\"small\" @click=\"addCart\">加入购物车</mt-button>\r\n          </p>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"mui-card-footer\">页脚</div>\r\n\t\t</div>\r\n    <!--商品参数区域-->\r\n    <div class=\"mui-card\">\r\n\t\t\t<div class=\"mui-card-header\">页眉</div>\r\n\t\t\t<div class=\"mui-card-content\">\r\n\t\t\t\t<div class=\"mui-card-content-inner\">\r\n\t\t\t\t\t商品参数\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"mui-card-footer\">页脚</div>\r\n\t\t</div>\r\n  </div>\r\n</template>\r\n<script>\r\n  import {Toast} from 'mint-ui'\r\n  //导入图片轮播组件\r\n  import swiper from \"../subcomponents/swiper.vue\"\r\n  export default {\r\n    data(){\r\n      return {\r\n        list:[],\r\n        goodsinfo:{},  //创建对象保存返回数据\r\n        lid:this.$route.params.lid,  //新闻列表编号\r\n        count:1\r\n      }\r\n    },\r\n    created(){\r\n      this.getImageList(),\r\n      this.getGoodsInfo()\r\n    },\r\n    methods:{\r\n      addCart(){\r\n        //调用vue实例对象操作共享数据方法\r\n        this.$store.commit(\"increment\");\r\n        //查询购物车中的共享数据\r\n        //console.log(this.$store.state.count)\r\n      },\r\n      getImageList(){\r\n        this.$http.get(\"imagelist/list\")\r\n        .then(result=>{\r\n          var obj=result.body;\r\n          if(obj.code == 1){  //返回成功数据\r\n            this.list=obj.msg;\r\n          }else{\r\n            Toast(\"数据加载失败\")  //脚手架 \r\n          }\r\n        })\r\n      },\r\n      getGoodsInfo(){\r\n        var url=\"goodslist/find?lid=\"+this.lid\r\n        this.$http.get(url).then(result=>{\r\n          this.goodsinfo=result.body.msg[0]\r\n        })\r\n      },\r\n      add(){\r\n        this.count++\r\n      },\r\n      minus(){\r\n        this.count--\r\n      }\r\n    },\r\n    //注册子组件\r\n    components:{\"swiper-box\":swiper}\r\n  }\r\n\r\n</script>\r\n<style>\r\n  .goods_info{\r\n    background-color:#eee;\r\n    overflow:hidden;\r\n  }\r\n  .goods_info{\r\n    background-color:#eee;\r\n    overflow:hidden;\r\n  }\r\n  .goods_info .now_price{\r\n    color:red;\r\n    font-size:16px;\r\n    font-weight:bold;\r\n  }\r\n</style>"]}]}