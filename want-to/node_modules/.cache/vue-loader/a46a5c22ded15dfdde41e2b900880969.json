n            <div class=\"mui-card-header\">{{goodsInfo.title}}</div>\r\n                <div class=\"mui-card-content\">\r\n                    <div class=\"mui-card-content-inner\">\r\n                       <p>\r\n                        市场价：<del>￥ {{goodsInfo.price_new}}</del>\r\n                        销售价：<span class=\"now_price\">\r\n                            ￥{{goodsInfo.price_old}}\r\n                        </span>\r\n                       </p>\r\n                       <p>\r\n                        <div class=\"mui-numbox\">\r\n                            <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"jian\" v-show=\"count>1\">-</button>\r\n                            <input id=\"box\" class=\"mui-input-numbox\" type=\"number\" v-model=\"count\"/>\r\n                            <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"add\">+</button>\r\n                        </div>\r\n                       </p>\r\n                       <p>\r\n                            <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n                            <mt-button type=\"danger\" size=\"small\">加入购物车</mt-button>\r\n                       </p>\r\n                    </div>\r\n                </div>\r\n            <div class=\"mui-card-footer\">页脚</div>\r\n        </div>\r\n        <!--商品参数区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">页眉</div>\r\n                <div class=\"mui-card-content\">\r\n                    <div class=\"mui-card-content-inner\">\r\n                       商品参数区域\r\n                    </div>\r\n                </div>\r\n            <div class=\"mui-card-footer\">页脚</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    //1.加载Toast组件\r\n    import swiper from \"../subcomponents/swiper.vue\";\r\n   \r\n    //2.创建vue实例\r\n    export default{\r\n        data:function(){\r\n            return {\r\n                list:[],\r\n                lid:this.$route.params.lid,\r\n                goodsInfo:{},\r\n                count:1\r\n            }\r\n          },\r\n         created(){\r\n               this.getImageList();\r\n               this. getGoodsInfo();\r\n           },\r\n        methods:{\r\n            add(){\r\n                this.count++;\r\n            },\r\n            jian(){\r\n                this.count--;\r\n            },\r\n             getImageList(){\r\n                 this.$http.get(\"http://localhost:3000/imagelist/list\").then(result=>{\r\n                     var obj = result.body;\r\n                     if(obj.code == 1){\r\n                        this.list = obj.msg;\r\n                     }else{\r\n                         Toast(\"数据加载失败\");\r\n                     }\r\n                 })\r\n             },\r\n             getGoodsInfo(){\r\n                 var lid = this.lid;\r\n                 this.$http.get(`goodslist/find?lid=${lid}`).then(result=>{\r\n                     if(result.body.code == 1){//追加数据\r\n                        this.goodsInfo = result.body.msg[0];\r\n                    }else{\r\n                        Toast(\"网络故障请检查\");\r\n                    }\r\n                 });\r\n             }\r\n            }, \r\n        \r\n        //注册子组件\r\n        components:{\r\n            \"swiper-box\":swiper\r\n        }\r\n        \r\n    }\r\n</script>\r\n<style>\r\n  .goods-info{\r\n      background:#eee;\r\n      overflow:hidden;\r\n  }\r\n  .goods-info .now_price{\r\n      color:red;\r\n      font-size:16px;\r\n      font-weight:bold;\r\n  }\r\n</style>\r\n"]}]}