rcesContent":["<template>\r\n  <div>\r\n    <h1>新闻列表</h1>\r\n    <ul class=\"mui-table-view\">\r\n\t\t\t\t<li class=\"mui-table-view-cell mui-media\" v-for=\"item in newslist\">\r\n\t\t\t\t\t<router-link :to=\"'/home/newsinfo/'+item.id\" >\r\n\t\t\t\t\t\t<img class=\"mui-media-object mui-pull-left\" :src=\"item.img_url\" :key=\"item.id\">\r\n\t\t\t\t\t\t<div class=\"mui-media-body\">\r\n\t\t\t\t\t\t\t<h1>{{item.title}}</h1>\r\n\t\t\t\t\t\t\t<p class='mui-ellipsis'>\r\n                <span>{{item.add_time | datatimeFilter}}</span>\r\n                <span>点击:{{item.click}}</span>\r\n              </p>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</router-link>\r\n\t\t\t\t</li>\r\n\t\t\t</ul>\r\n  </div>\r\n</template>\r\n<script>\r\n  import {Toast} from 'mint-ui'\r\n  export default{\r\n    data(){\r\n      return {\r\n        newslist:[]\r\n      }\r\n    },\r\n    created(){\r\n      this.getNewsList();\r\n    },\r\n    methods:{\r\n      getNewsList(){\r\n        //将发送ajax请求服务器保存到变量里\r\n        this.$http.get(\"newslist/list\")\r\n        .then(result=>{\r\n          var obj=result.body;\r\n          if(obj.code == 1){  //返回成功数据\r\n            this.newslist=obj.msg\r\n          }else{ \r\n            Toast(\"数据加载失败\")\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style>\r\n  .mui-table-view h1{\r\n    font-size:14px;\r\n  }\r\n  .mui-table-view .mui-ellipsis{\r\n    color:#26a2ff;\r\n    font-size:12px;\r\n    display:flex;\r\n    justify-content:space-between;/*点击次数,右侧 */\r\n  }\r\n</style>"],"sourceRoot":"src\\components\\news"}]}