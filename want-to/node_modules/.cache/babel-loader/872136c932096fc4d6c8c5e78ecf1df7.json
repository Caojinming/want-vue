Comment() {\n      var _this2 = this;\n\n      //发表评论\n      // 1.创建正则验证内容\n      var m = this.msg.trim(); // 2.如果格式不正确，提示  \n\n      if (m.length < 3) {\n        Toast(\"你发表的内容过少，必须大于3个字符\");\n        return;\n      } // 3.如果格式正确，发送ajax请求\n\n\n      var n = this.id;\n      var url = \"commentlist/postcomment?nid=\".concat(n, \"&content=\").concat(m, \"&username=lily\");\n      this.$http.get(url).then(function (result) {\n        if (result.body.code == 1) {\n          Toast(result.body.msg);\n          _this2.msg = \"\";\n        }\n      }); // 4.判断\n    }\n  },\n  props: [\"id\"]\n};",{"version":3,"sources":["comment.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;AAoBA,SAAA,KAAA,QAAA,SAAA;AACA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,WAAA,EADA;AAEA,WAAA,CAFA;AAGA,YAAA;AAHA,KAAA;AAKA,GAPA;AAQA,SARA,qBAQA;AACA;AACA,SAAA,WAAA;AACA,GAXA;AAYA,WAAA;AACA,WADA,qBACA;AACA;AACA;AACA,WAAA,GAAA,GAHA,CAIA;;AACA,WAAA,WAAA;AACA,KAPA;AAQA,eARA,yBAQA;AAAA;;AACA;AACA,UAAA,MAAA,KAAA,EAAA;AACA,UAAA,IAAA,KAAA,GAAA,CAHA,CAIA;;AACA,UAAA,qCAAA,GAAA,kBAAA,CAAA,CAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,YAAA,OAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AAAA;AACA,gBAAA,IAAA,GAAA,MAAA,IAAA,CAAA,MAAA,CAAA,OAAA,IAAA,CAAA,GAAA,CAAA;AACA,SAFA,MAEA;AACA,gBAAA,SAAA;AACA;AACA,OANA;AAOA,KArBA;AAsBA,eAtBA,yBAsBA;AAAA;;AAAA;AACA;AACA,UAAA,IAAA,KAAA,GAAA,CAAA,IAAA,EAAA,CAFA,CAGA;;AACA,UAAA,EAAA,MAAA,GAAA,CAAA,EAAA;AACA,cAAA,mBAAA;AACA;AACA,OAPA,CAQA;;;AACA,UAAA,IAAA,KAAA,EAAA;AACA,UAAA,4CAAA,CAAA,sBAAA,CAAA,mBAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,YAAA,OAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,gBAAA,OAAA,IAAA,CAAA,GAAA;AACA,iBAAA,GAAA,GAAA,EAAA;AACA;AACA,OALA,EAXA,CAiBA;AAEA;AAzCA,GAZA;AAuDA,SAAA,CAAA,IAAA;AAvDA,CAAA","sourcesContent":["<template>\r\n    <div class=\"cmt-container\">\r\n        <h3>发表评论</h3>\r\n        <hr/>\r\n        <textarea placeholder=\"请输入评论内容(最多120字)\" maxlength=\"120\" v-model=\"msg\"></textarea>\r\n        <mt-button type=\"primary\" size=\"large\" @click=\"postComment\">发表</mt-button>\r\n        <div class=\"cmt-list\">\r\n            <div class=\"cmt-item\" v-for=\"(item,i) in list\" :key=\"item.id\">\r\n                <div class=\"cmt-title\">\r\n                    第{{i+1}}楼&nbsp;&nbsp;用户：{{item.username}} &nbsp;发表时间 {{item.add_time | datetimeFilter}}\r\n                </div>\r\n                <div class=\"cmt-body\">\r\n                    {{item.content}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <mt-button type=\"danger\" size=\"large\" @click=\"getMore\">加载更多</mt-button>\r\n    </div> \r\n</template>\r\n<script>\r\n    import {Toast} from 'mint-ui'\r\n    export default{\r\n        data(){\r\n            return {   \r\n                msg:\"\",\r\n                pno:1,\r\n                list:[]\r\n            }\r\n        },\r\n       created(){\r\n            //console.log(\"父组件传递值\"+this.id);\r\n            this.getComments();\r\n        },\r\n        methods:{\r\n            getMore(){\r\n                //加载更多\r\n                //1.当前页+1\r\n                this.pno++;\r\n               // 2.调用查询方法\r\n                this.getComments();\r\n            },\r\n            getComments(){\r\n                //获取两个参数\r\n                var nid = this.id;\r\n                var p = this.pno; \r\n                //发送ajax请求获取当前数据\r\n                var url = `commentlist/list?nid=${nid}&pno=${p}`;\r\n                this.$http.get(url).then(result=>{\r\n                    if(result.body.code == 1){//追加数据\r\n                        this.list = this.list.concat(result.body.msg);\r\n                    }else{\r\n                        Toast(\"网络故障请检查\");\r\n                    }\r\n                });       \r\n            },\r\n            postComment(){//发表评论\r\n                // 1.创建正则验证内容\r\n                var m = this.msg.trim();\r\n                // 2.如果格式不正确，提示  \r\n                if(m.length < 3){\r\n                    Toast(\"你发表的内容过少，必须大于3个字符\");\r\n                    return;\r\n                }\r\n                // 3.如果格式正确，发送ajax请求\r\n                var n = this.id;\r\n               var url = `commentlist/postcomment?nid=${n}&content=${m}&username=lily`;\r\n               this.$http.get(url).then(result=>{\r\n                   if(result.body.code == 1){\r\n                       Toast(result.body.msg);\r\n                       this.msg = \"\";\r\n                   }\r\n               })\r\n                // 4.判断\r\n                \r\n            }\r\n        },\r\n        props:[\"id\"]\r\n    }\r\n</script>\r\n<style>\r\n    .cmt-container h3{\r\n        font-size:18px;\r\n    }\r\n    .cmt-container textarea{\r\n        font-size:14px;\r\n        height:85px;\r\n        margin:0;\r\n    }\r\n    .cmt-container .cmt-list{\r\n        margin:5px 0;\r\n    }\r\n    .cmt-container .cmt-list .cmt-item{\r\n        font-size:13px;\r\n    }\r\n    .cmt-container .cmt-list .cmt-item .cmt-title{\r\n        line-height:30px;\r\n        background-color:#ccc;\r\n    }\r\n    .cmt-container .cmt-list .cmt-item .cmt-body{\r\n        line-height:30px;\r\n        text-indent:2em;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src\\components\\subcomponents"}]}